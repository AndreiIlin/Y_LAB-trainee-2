{
  "name": "react-skeleton",
  "version": "2.1.2",
  "description": "YLab React Skeleton",
  "main": "index.client.tsx",
  "scripts": {
    "start": "vite serve",
    "start:ssr": "cross-env NODE_ENV=development node ./ssr/index.js",
    "build": "npm run build:client & npm run build:server",
    "build:client": "vite build --outDir ../dist/client",
    "build:server": "vite build --outDir ../dist/server --ssr index.server.tsx",
    "preview": "vite preview",
    "preview:ssr": "node ./ssr/index.js",

    "wstart": "cross-env webpack serve",
    "wstart:ssr": "npm run build:all & cross-env node ./server.js",
    "wbuild": "cross-env NODE_ENV=production webpack",
    "wbuild:ssr": "cross-env NODE_ENV=production TARGET=node webpack",
    "wbuild:all": "npm run build & npm run build:ssr",
    "wbuild:analize": "cross-env NODE_ENV=production BUILD_ANALYZE=true webpack",
    "server": "node ./server.js",
    "test": "jest",
    "test:eslint": "./node_modules/.bin/eslint ./"
  },
  "type": "module",
  "private": true,
  "author": "Vladimir Shestakov boolive@yandex.ru",
  "license": "ISC",
