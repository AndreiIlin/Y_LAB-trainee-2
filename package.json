{
  "name": "react-skeleton",
  "version": "3.0.1",
  "description": "YLab React Skeleton",
  "main": "index.tsx",
  "scripts": {
    "start": "vite serve",
    "start:ssr": "cross-env NODE_ENV=development node ./ssr/index.js",
    "build": "npm run build:client & npm run build:server",
    "build:client": "vite build --outDir ../dist/client",
    "build:server": "vite build --outDir ../dist/server --ssr root.tsx",
    "preview:client": "npm run build:client && vite preview",
    "preview:ssr": "npm run build && node ./ssr/index.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "test:eslint": "./node_modules/.bin/eslint ./",
    "lint": "eslint ./src"
  },
  "type": "module",
  "author": "Vladimir Shestakov boolive@yandex.ru",
  "license": "ISC",
  "dependencies": {
    "@bem-react/classname": "^1.6.0",
    "ajv": "^8.12.0",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "history": "^5.3.0",
    "html-dom-parser": "^4.0.0",
    "htmlparser2": "^9.0.0",
    "js-base64": "^3.7.5",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "merge-change": "^1.8.1",
    "path-to-regexp": "^6.2.1",
    "prop-types": "^15.8.1",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.13.0",
    "react-router-dom": "^6.13.0",
    "shallowequal": "^1.1.0",
    "stream-replace": "^1.0.0",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.13",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router": "^5.1.20",
    "@types/express": "^4.17.17",
    "@types/lodash.get": "^4.4.7",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "http-proxy": "^1.18.1",
    "jest": "^29.5.0",
    "prettier": "2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0"
  }
}
